[paths]
source =
   cashews

[run]
branch = true
source =
    cashews

parallel = true
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover
    ...

    # Don't complain about missing debug-only code:
    def __repr__

    # Don't complain about abstract methods or overloads, they aren't run:
    @(abc\.)?abstractmethod
    @overload

[report]
precision = 2
fail_under = 70
show_missing = true
